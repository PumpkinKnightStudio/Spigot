From 57360e2694ba8975c332da7a38b6ba87836b22fc Mon Sep 17 00:00:00 2001
From: Wouter <wouterdoeland@gmail.com>
Date: Tue, 31 Jan 2017 21:21:44 +0100
Subject: [PATCH] Added getters to CustomTimingsHandler.java


diff --git a/src/main/java/org/spigotmc/CustomTimingsHandler.java b/src/main/java/org/spigotmc/CustomTimingsHandler.java
index 8d98297..348e4f5 100644
--- a/src/main/java/org/spigotmc/CustomTimingsHandler.java
+++ b/src/main/java/org/spigotmc/CustomTimingsHandler.java
@@ -162,4 +162,68 @@ public class CustomTimingsHandler
         start = 0;
         timingDepth = 0;
     }
+
+    /**
+     * Returns the name of this timer.
+     * @return the name of this timer
+     */
+    public String getName() {
+        return name;
+    }
+
+    /**
+     * Returns the count of this timer.
+     * @return the count of this timer
+     */
+    public long getCount() {
+        return count;
+    }
+
+    /**
+     * Returns the starting tick of this timer.
+     * @return the starting tick of this timer
+     */
+    public long getStart() {
+        return start;
+    }
+
+    /**
+     * Returns the timing depth of this timer.
+     * @return the timing depth of this timer
+     */
+    public long getTimingDepth() {
+        return timingDepth;
+    }
+
+    /**
+     * Returns the total time of this timer
+     * @return the total time of this timer
+     */
+    public long getTotalTime() {
+        return totalTime;
+    }
+
+    /**
+     * Returns the current total ticks of this timer.
+     * @return total amount of ticks of this timer
+     */
+    public long getCurTickTotal() {
+        return curTickTotal;
+    }
+
+    /**
+     * Returns the violation count of this timer.
+     * @return the amount of violations for this timer
+     */
+    public long getViolations() {
+        return violations;
+    }
+
+    /**
+     * Returns the queue containing all timers.
+     * @return queue containing all timers
+     */
+    public static Queue<CustomTimingsHandler> getHANDLERS() {
+        return HANDLERS;
+    }
 }
